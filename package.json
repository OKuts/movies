{
    "name": "movies",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --watchAll --verbose",
        "eject": "react-scripts eject",
        "start": "npm run build && serve -s build",
        "format": "prettier-eslint --list-different $PWD/'src/**/*.tsx'; prettier-eslint --write $PWD/'src/**/*.tsx'",
        "type-check": "tsc --noEmit -p tsconfig.json",
        "lint": "eslint ./src/** --ext .{ts,tsx} -c .eslintrc --no-error-on-unmatched-pattern",
        "lint:fix": "eslint ./src/** --ext .{ts,tsx} -c .eslintrc --no-error-on-unmatched-pattern --fix",
        "validate": "npm run type-check && npm run lint"
    },
    "engines": {
        "node": "14.x"
    },
    "dependencies": {
        "@ant-design/icons": "4.6.2",
        "@hookform/resolvers": "2.4.0",
        "antd": "4.16.6",
        "axios": "0.21.1",
        "clsx": "1.1.1",
        "date-fns": "2.22.1",
        "history": "5.0.0",
        "mobx": "6.3.0",
        "mobx-react-lite": "3.2.0",
        "react": "17.0.2",
        "react-content-loader": "6.0.3",
        "react-dom": "17.0.2",
        "react-hook-form": "7.4.2",
        "react-query": "3.15.2",
        "react-router-dom": "6.0.0-beta.0",
        "react-toastify": "7.0.4",
        "sanitize.css": "12.0.1",
        "styled-components": "5.3.0",
        "uuid": "8.3.2",
        "yup": "0.32.9"
    },
    "devDependencies": {
        "@sotream/eslint-config-react-ts": "0.1.1",
        "@testing-library/jest-dom": "5.12.0",
        "@testing-library/react": "11.2.6",
        "@testing-library/react-hooks": "7.0.0",
        "@testing-library/user-event": "13.1.8",
        "@types/jest": "26.0.23",
        "@types/node": "15.0.2",
        "@types/react": "17.0.5",
        "@types/react-dom": "17.0.3",
        "@types/styled-components": "5.1.10",
        "@types/uuid": "8.3.0",
        "eslint": "7.26.0",
        "eslint-config-prettier": "8.3.0",
        "nock": "13.1.0",
        "prettier-eslint": "12.0.0",
        "prettier-eslint-cli": "5.0.1",
        "react-scripts": "4.0.3",
        "sass": "1.32.12",
        "serve": "11.3.2",
        "stylelint": "13.13.1",
        "stylelint-config-recess-order": "2.4.0",
        "stylelint-config-standard": "22.0.0",
        "stylelint-declaration-block-no-ignored-properties": "2.3.0",
        "stylelint-order": "4.1.0",
        "stylelint-scss": "3.19.0",
        "stylelint-selector-no-empty": "1.0.8",
        "stylelint-use-nesting": "3.0.0",
        "ts-node": "9.1.1",
        "typescript": "4.2.4"
    }
}
